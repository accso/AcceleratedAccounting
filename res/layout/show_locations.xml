<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/AccsoLinearVerticalLayout" >

    <TextView
        style="@style/AccsoHeading"
        android:text="@string/show_locations_heading" />
    
        
   
	
    <TableLayout
        android:id="@+id/show_locations_add_location"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:stretchColumns="1"
        android:paddingBottom="15dp" >
    </TableLayout>

    <View android:layout_height="1px" android:layout_width="match_parent" android:background="@color/black"> </View>
    <ListView
        android:id="@+id/show_locations_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
    </ListView>
    <View android:layout_height="1px" android:layout_width="match_parent" android:background="@color/black"> </View>

</LinearLayout>